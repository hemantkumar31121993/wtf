decl
	integer m;
	integer factorial(integer k,l;);
	integer swap(integer &a,&b;);
	integer marx(integer &m;);
	integer karl(integer &k;);
	integer kara[3];
enddecl

integer factorial(integer k,l;) {
	decl
		integer n;
	enddecl
	begin	
		write(k);
		if (k==1) then
			n=1;
		else
			n = k * factorial(k-1,l);
		endif;
		return n;
	end
}

integer marx(integer &m;) {
	decl
	enddecl
	begin
		m = 40;
		return 0;
	end
}

integer karl(integer &k;) {
	decl
	enddecl
	begin
		return marx(k);
	end
}

integer swap(integer &a,&b;) {
	decl
	enddecl
	begin
		a = a + b;
		b = a - b;
		a = a - b;
		return 0;
	end
}

integer main() {
	decl
		integer k,a,b;
	enddecl
	begin	
		k=factorial(factorial(3,2),2);
		write(k);
		return 0;
	end
}

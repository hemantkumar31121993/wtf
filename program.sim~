START
MOV SP, 2048
MOV BP, SP
JMP MAIN
swapfact:
PUSH BP
MOV BP, SP
MOV R0, SP
MOV R1, 2
ADD R0, R1
MOV SP, R0
MOV R1, BP
MOV R3, -5
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
OUT R0
MOV R1, BP
MOV R3, -4
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
OUT R0
MOV R1, BP
MOV R3, -4
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV [R1], R0
MOV R1, BP
MOV R3, -5
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
MOV R1, BP
MOV R2, -4
ADD R1, R2
MOV R3, [R1]
MOV [R3], R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R0, [R1]
MOV R1, BP
MOV R2, -5
ADD R1, R2
MOV R3, [R1]
MOV [R3], R0
MOV R1, BP
MOV R3, -5
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
OUT R0
MOV R1, BP
MOV R3, -4
ADD R1, R3
MOV R2, [R1]
MOV R0, [R2]
OUT R0
MOV R1, BP
MOV R2, -3
ADD R1, R2
MOV R0, [R1]
MOV R1, 1
EQ R0, R1
JZ R0, LABEL0
MOV R0, 1
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV [R1], R0
JMP LABEL1
LABEL0: MOV R1, BP
MOV R2, -3
ADD R1, R2
MOV R0, [R1]
PUSH R0
MOV R1, BP
MOV R3, -5
ADD R1, R3
MOV R2, [R1]
PUSH R2
MOV R1, BP
MOV R3, -4
ADD R1, R3
MOV R2, [R1]
PUSH R2
MOV R2, BP
MOV R3, -3
ADD R2, R3
MOV R1, [R2]
MOV R2, 1
SUB R1, R2
PUSH R1
PUSH R0
CALL swapfact
MOV R1, [SP]
MOV R2, SP
MOV R3, 4
SUB R2, R3
MOV SP, R2
POP R0
MUL R0, R1
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV [R1], R0
LABEL1: MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV R0, [R1]
MOV R1, BP
MOV R2, 2
SUB R1, R2
MOV [R1], R0
MOV SP, BP
POP BP
RET
MAIN :
MOV R0, SP
MOV R1, 4
ADD R0, R1
MOV SP, R0
MOV R0, 1
MOV [0], R0
MOV R0, 5
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV [R1], R0
MOV R0, 6
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV [R1], R0
IN R0
MOV R1, BP
MOV R2, 3
ADD R1, R2
MOV [R1], R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R3, [R1]
PUSH R3
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV R3, [R1]
PUSH R3
MOV R2, BP
MOV R3, 3
ADD R2, R3
MOV R1, [R2]
PUSH R1
PUSH R0
CALL swapfact
MOV R0, [SP]
MOV R1, SP
MOV R2, 4
SUB R1, R2
MOV SP, R1
MOV R1, BP
MOV R2, 4
ADD R1, R2
MOV [R1], R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R0, [R1]
OUT R0
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV R0, [R1]
OUT R0
MOV R1, BP
MOV R2, 4
ADD R1, R2
MOV R0, [R1]
OUT R0
MOV R0, 0
MOV R1, BP
MOV R2, 2
SUB R1, R2
MOV [R1], R0
MOV R0, SP
MOV R1, 4
SUB R0, R1
MOV SP, R0
JMP EXIT
EXIT:
HALT

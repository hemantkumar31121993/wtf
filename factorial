decl
	integer m;
	integer factorial(integer k;);
	integer swap(integer &a,&b;);
enddecl

integer factorial(integer k;) {
	decl
		integer n;
	enddecl
	begin	
		if (k==1) then
			n=1;
		else
			n = k * factorial(k-1);
		endif;
		return n;
	end
}

integer swap(integer &a,&b;) {
	decl
	enddecl
	begin
		a = a + b;
		b = a - b;
		a = a - b;
		return 0;
	end
}

integer main() {
	decl
		integer k,a,b;
	enddecl
	begin	
		a = 7;
		b = 17;
		write(a);
		write(b);
		k = swap(a,b);
		write(a);
		write(b);
		k=factorial(6);
		write(k);
		return 0;
	end
}
